{
    "name": "cardgameforge",
    "version": "0.1.0",
    "scripts": {
        "build:server": "tsc -p tsconfig.server.json",
        "build:client": "tsc -p tsconfig.client.json",
        "dev:server": "tsc --watch -p tsconfig.server.json & nodemon dist/server/server/index.js",
        "dev:client": "tsc --watch -p tsconfig.client.json",
        "start:server": "node dist/server/server/index.js",
        "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,vue,html,css,scss,md}\"",
        "lint": "eslint --ext .ts,.tsx src",
        "prepare": "husky install",
        "test": "jest --config jest.config.ts"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.4.7",
        "@mui/material": "^6.4.6",
        "@reduxjs/toolkit": "^2.6.0",
        "express": "^4.18.2",
        "notistack": "^3.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.2.0",
        "seedrandom": "^3.0.5",
        "socket.io": "^4.6.0",
        "socket.io-client": "^4.8.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.6.3",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/seedrandom": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^8.24.0",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^5.0.0",
        "globals": "^15.12.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "prettier": "2.7.1",
        "pretty-quick": "^3.1.3",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.0",
        "vite": "^6.0.1"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        "./server": "./dist/server/server/index.js",
        "./client": "./dist/client/client/index.js"
    }
}
